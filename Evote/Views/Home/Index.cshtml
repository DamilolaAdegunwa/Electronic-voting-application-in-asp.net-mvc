<!DOCTYPE html>
<html>
<head>
    <title>Module Pricing Table Flat Responsive Widget Template :: w3layouts</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="keywords" content="Vote, choose, pick, select" />
    <script type="application/x-javascript"> 
        addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); }
    </script>
    <!-- Custom Theme files -->
    <link href="~/choosecss/style.css" rel="stylesheet" />
    <!-- //Custom Theme files -->
    <!-- web font -->
    <link href='//fonts.googleapis.com/css?family=Open+Sans:400,300,600,700,800' rel='stylesheet' type='text/css'>
    <!-- //web font -->
</head>
<body style="background-image: url('../Loginimages/4.jpg')">
    <!-- main -->
    <div class="main">
        <h1>Module Pricing Table</h1>
        <img src="~/Home/GetFile/4" />
        @foreach (var item in db.Contestants)
        {
            <div class="main-row">
                <div class="pricing-grid agileits-shadow">
                    <div class="agileinfo-price ">
                        <h3>@item.fullname<br> @item.membership_number</h3>
                    </div>
                    <div class="w3ls-main" style="width:100%">
                        <div id="cube" class="show-front" style="width:100%">
                            <figure class="top" style="width:100%"> </figure>
                        </div>
                    </div>
                    <div class="price-bg">
                        <div class="col-sm-6" @*style="background-image: url(/Home/GetFile/@item.Id);"*@>
                            <img src="~/Home/GetFile/@item.Id" />
                        </div>
                        <div class="col-sm-6">
                            <p class="price-label-1"><span>@item.post</span></p>
                            <ul class="count">
                                <li> @item.manifesto </li>
                                @*<li> One week (7 days)</li>
                                  <li> You can add more guides or extend the duration of your subscription as you need</li>*@
                            </ul>
                        </div>
                        
                        <div class="buy">
                            <a class="popup-with-zoom-anim" href="#small-dialog">SIGN UP</a>
                        </div>
                    </div>
                    <div class="price-bg">

                    </div>
                </div>
                <div class="clear"> </div>
            </div>
            <br />
        }
        @*<br/>
        <div class="main-row">
            <!-- pricing-table-two -->
            <div class="pricing-grid agileits-shadow">

                <div class="agileinfo-price two">
                    <h3>MONTHLY <br>MODULE</h3>

                </div>
                <div class="w3ls-main">
                    <div id="cube2" class="show-front" style="width:100%">
                        <figure class="top top-two"> </figure>
                    </div>
                </div>
                <div class="price-bg">
                    <p class="price-label-2"><span>$19.99</span></p>
                    <ul class="count">
